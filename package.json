        "js-cookie": "^3.0.5",
        "mockjs": "^1.1.0",
        "next": "13.4.19",
        "nprogress": "^0.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-redux": "^8.1.2"
    },
    "devDependencies": {
        "@svgr/webpack": "^8.1.0",
        "@types/js-cookie": "^3.0.3",
        "@types/mockjs": "^1.0.7",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
